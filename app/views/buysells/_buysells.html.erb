<div id="wrap">
<table class="table table-hover">
	  <thead>
	    <tr>
	      <th><input type="checkbox" name="select-all" id="select-all"></th>
	      <th>Link</th>
	      <th>Nome</th>
	      <th>Vendedor</th>
	      <th>Comprador</th>
	      <th>Item</th>
	      <th>Valor</th>
	      <th>Data de Criação</th>
	    </tr>
	  </thead>
	  <tbody>
	  <% obj.each do |buysell| %>
	  	<%if logged_in? && current_user.id == buysell.user_id %>
	    <tr>
	      
	      <td><%= check_box_tag "aluguels_ids[]", buysell.id, false, :id => "#{buysell.id}" %></td>
	      
	      <td><%= link_to 'Doc', buysell.document.uploaded_file.url %></td>
	      
	      <td><%= buysell.document.name %></td>

	      <td><%= buysell.vendedor %></td>

	      <td><%= buysell.comprador %></td>

	      <td><%= buysell.item %></td>

	      <td><%= buysell.valor %></td>

	      <td><%= buysell.document.created_at %></td>
	    </tr>
	    <% end %>
	  <% end %>
	  </tbody>
</table>
</div>

