<div id="wrap">
<table class="table table-hover">
	  <thead>
	    <tr>
	      <th><input type="checkbox" name="select-all" id="select-all"></th>
	      <th>Link</th>
	      <th>Nome</th>
	      <th>Imovel</th>
	      <th>Endereço</th>
	      <th>Locador</th>
	      <th>Locatário</th>
	      <th>Valor</th>
	      <th>Data de Criação</th>
	    </tr>
	  </thead>
	  <tbody>
	  <% obj.each do |aluguel| %>
	  	<%if logged_in? && current_user == aluguel.user_id %>
	    <tr>
	      
	      <td><%= check_box_tag "aluguels_ids[]", aluguel.id, false, :id => "#{aluguel.id}" %></td>
	      
	      <td><%= link_to 'Doc', aluguel.document.uploaded_file.url %></td>
	      
	      <td><%= aluguel.document.name %></td>

	      <td>
	        <%if aluguel.imovel == "Residencial" %>
	      		<span class="label label-primary">Residencial</span>
	      	<%elsif aluguel.imovel == "Comercial" %>
	      		<span class="label label-warning">Comercial</span>
	      	<% end %>
	      </td>

	      <td><%= aluguel.local %></td>

	      <td><%= aluguel.locador %></td>

	      <td><%= aluguel.locatario %></td>

	      <td><%= aluguel.valor %></td>

	      <td><%= aluguel.document.created_at %></td>

	  <% end %>
	  <% end %>
	  </tbody>
</table>
</div>
